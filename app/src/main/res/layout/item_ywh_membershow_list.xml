<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="28px"
    android:background="@color/white"
    android:orientation="vertical"
    android:paddingBottom="43px"
    android:paddingLeft="46px"
    android:paddingRight="46px"
    android:paddingTop="43px">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            style="@style/ywh_member_tv1"
            android:text="姓            名："/>

        <TextView
            android:id="@+id/tv_name"
            style="@style/ywh_member_tv2"
            android:text=""
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="34px"
        android:orientation="horizontal">

        <TextView
            style="@style/ywh_member_tv1"
            android:text="房 屋 信 息 ："/>

        <TextView
            android:id="@+id/tv_house_message"
            style="@style/ywh_member_tv2"
            android:text=""/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="34px"
        android:orientation="horizontal">

        <TextView
            style="@style/ywh_member_tv1"
            android:text="身份证号码："/>

        <TextView
            android:id="@+id/tv_idcard"
            style="@style/ywh_member_tv2"
            android:text=""/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="34px"
        android:orientation="horizontal">

        <TextView
            style="@style/ywh_member_tv1"
            android:text="身            份："/>

        <TextView
            android:id="@+id/tv_identity"
            style="@style/ywh_member_tv2"
            android:text=""/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="34px"
        android:orientation="horizontal">

        <TextView
            style="@style/ywh_member_tv1"
            android:text="工 作 单 位 ："/>

        <TextView
            android:id="@+id/tv_work_place"
            style="@style/ywh_member_tv2"
            android:text=""/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="34px"
        android:orientation="horizontal">

        <TextView
            style="@style/ywh_member_tv1"
            android:text="工 作 职 位 ："/>

        <TextView
            android:id="@+id/tv_work"
            style="@style/ywh_member_tv2"
            android:text=""/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="34px"
        android:orientation="horizontal">

        <TextView
            style="@style/ywh_member_tv1"
            android:text="其 它 职 位 ："/>

        <TextView
            android:id="@+id/tv_other"
            style="@style/ywh_member_tv2"
            android:text=""/>
    </LinearLayout>

    <TextView
        android:id="@+id/tv_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="34px"
        android:background="@color/color_f5f5f5"
        android:lineSpacingExtra="20px"
        android:padding="34px"
        android:text=""
        android:textColor="@color/umeng_socialize_text_share_content"
        android:textSize="40px"/>

</com.zhy.autolayout.AutoLinearLayout>