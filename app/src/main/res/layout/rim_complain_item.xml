<?xml version="1.0" encoding="utf-8"?>
<com.yxld.yxchuangxin.view.AutoCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="40px"
    android:layout_marginRight="40px"
    android:layout_marginTop="26px"
    android:id="@+id/item_main"
    android:clickable="true"
    app:cardCornerRadius="14px"
    app:cardElevation="8px">

    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_complaintBianhaotag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="38px"
            android:textColor="@color/color_909090"
            android:text="订单编号:"
            android:layout_marginLeft="40px"
            android:layout_marginTop="46px"
            android:layout_alignParentLeft="true"
            />

        <TextView
            android:id="@+id/tv_complaintBianhao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="38px"
            android:textColor="@color/cb_textcolor_checked"
            android:text="1000619274450811"
            android:layout_toRightOf="@id/tv_complaintBianhaotag"
            android:layout_marginTop="46px"
            android:layout_marginLeft="28px"/>

        <TextView
            android:id="@+id/tv_complaintState"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="40px"
            android:textColor="@color/cb_textcolor_unchecked"
            android:text="待处理"
            android:layout_marginRight="40px"
            android:layout_marginTop="44px"
            android:layout_alignParentRight="true"
            />


        <TextView
            android:id="@+id/tv_complaintTimetag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="38px"
            android:textColor="@color/color_909090"
            android:text="投诉时间:"
            android:layout_marginLeft="40px"
            android:layout_marginTop="30px"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/tv_complaintBianhaotag"
            />

        <TextView
            android:id="@+id/tv_complaintTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="38px"
            android:textColor="@color/cb_textcolor_checked"
            android:text="2017-5-28 12:51:28"
            android:layout_toRightOf="@id/tv_complaintTimetag"
            android:layout_marginLeft="28px"
            android:layout_below="@+id/tv_complaintBianhao"
            android:layout_marginTop="30px"/>


        <TextView
            android:id="@+id/tv_complaintcontenttag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="38px"
            android:textColor="@color/color_909090"
            android:text="投诉内容:"
            android:layout_marginLeft="40px"
            android:layout_marginTop="30px"
            android:layout_marginBottom="46px"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/tv_complaintTimetag"

            />

        <TextView
            android:id="@+id/tv_complaintcontent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="38px"
            android:textColor="@color/cb_textcolor_checked"
            android:text="送货太慢送货太慢送货太慢送货太慢送货太慢"
            android:layout_toRightOf="@id/tv_complaintcontenttag"
            android:layout_marginLeft="28px"
            android:layout_marginBottom="46px"
            android:maxEms="14"
            android:singleLine="true"
            android:ellipsize="end"
            android:layout_below="@+id/tv_complaintTime"
            android:layout_marginTop="30px"
            />

    </com.zhy.autolayout.AutoRelativeLayout>


</com.yxld.yxchuangxin.view.AutoCardView>
